---
// Components
// - Layout
import Layout from '../layouts/Layout.astro'
// - UI
import Hero from '../components/blocks/hero/PageHeader.astro'
import SimpleFeed from '../components/blocks/feeds/BasicFeed.astro'
import CTA from '../components/blocks/CTA/BasicLight.astro'

// Data
import feedData from '../data/json-files/changelogData.json'

// Content
// - SEO
const SEO = {
	title: 'AI Hair Extension | Этапы запуска платформы',
	description:
		'Как проходит внедрение AI Hair Extension: демо, брендинг, настройка токенов и запуск подписок. Пошаговый план с реальными сроками.'
}
// - Page Header
const header = {
	title: 'Преврати школу в бизнес-модель <strong>с пассивным доходом</strong>',
	text: 'Сроки этапов - условны, в твоем случае они могут быть другими. Возможно, быстрее.'
}
---

<Layout title={SEO.title} description={SEO.description}>
	<Hero title={header.title} text={header.text} />
	<SimpleFeed data={feedData} />
	<CTA
		sectionClasses="bg-neutral-100 dark:bg-neutral-900/40"
		title="Готовы пройти этот путь?"
		text="Покажем демо, обсудим задачи и составим дорожную карту внедрения именно под ваш бизнес."
		buttonLabel="Забронировать демо"
		buttonLink=""
		buttonBooking={true}
	/>
</Layout>
